@model IEnumerable<ProvidersApp.Classes.Query6Data>

@{
    ViewBag.Title = "Результаты запроса";
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            ФИО
        </th>
        <th>
            Телефон
        </th>
        <th>
            Адрес
        </th>
        <th>
            Количество минут
        </th>
        <th>
            Количество СМС
        </th>
        <th>
            Цена
        </th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @item.full_name
        </td>
        <td>
            @item.telephone_number
        </td>
        <td>
            @item.address
        </td>
        <td>
            @item.minute_number
        </td>
        <td>
            @item.sms_number
        </td>
        <td>
            @item.price
        </td>
    </tr>
    }
</table>