@model IEnumerable<ProvidersApp.Classes.Query2Data>

@{
    ViewBag.Title = "Результаты запроса";
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            Сумма
        </th>
        <th>
            ФИО
        </th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @item.summary
        </td>
        <td>
            @item.full_name
        </td>
    </tr>
    }
</table>