@model IEnumerable<ProvidersApp.Classes.Query9Data>

@{
    ViewBag.Title = "Результаты запроса";
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered table-hover">
    <tr>
        <th>
            Название
        </th>
        <th>
            Количество
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.name
            </td>
            <td>
                @item.cnt
            </td>
        </tr>
    }
</table>