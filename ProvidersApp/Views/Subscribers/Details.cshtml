@model ProvidersApp.Models.subscribers

@{
    ViewBag.Title = "Подробно";
}



<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            ФИО
        </dt>

        <dd>
            @Html.DisplayFor(model => model.full_name)
        </dd>

        <dt>
            Номер телефона
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telephone_number)
        </dd>

        <dt>
            Адрес
        </dt>

        <dd>
            @Html.DisplayFor(model => model.personal_data.address)
        </dd>

        <dt>
            Название региона
        </dt>

        <dd>
            @Html.DisplayFor(model => model.regions.name)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Вернуться к записям", "Index")
</p>
